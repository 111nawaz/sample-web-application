#!/usr/bin/env groovy

@Library('shared-library@master') _ //master or whatever branch

pipeline{

      
        
        stages{

              stage('build'){
		      
		      agent {
                	docker {
                	image 'maven'
                	args '-v $HOME/.m2:/root/.m2'
               		    }
           	 	}
		      
                  steps{
                      build ('Clean')
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 10)
                    }
                }
		
           }	       	     	         
}
