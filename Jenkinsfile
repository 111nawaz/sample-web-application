pipeline {
    agent { label 'jenkins-java' }

    stages {
        stage('clone'){
            steps {
                script {
                    echo "this is clone stage"

                }
            }
        }

        stage('build'){
            steps {
                script {
                        echo "this is build stage"
                }
            }
        }

        stage('deploy'){
            steps {
                script {
                        echo "this is build stage"
                }
            }
        }

    }
}
